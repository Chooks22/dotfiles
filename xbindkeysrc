"xrandr -o normal && feh --bg-fill --no-fehbg ~/.wallpaper/h/default"
  Control+Alt + Up

"xrandr -o inverted && feh --bg-fill --no-fehbg ~/.wallpaper/h/default"
  Control+Alt + Down

"xrandr -o left && feh --bg-fill --no-fehbg ~/.wallpaper/v/default"
  Control+Alt + Left

"xrandr -o right && feh --bg-fill --no-fehbg ~/.wallpaper/v/default"
  Control+Alt + Right

"amixer sset Master 5%+"
  XF86AudioRaiseVolume

"amixer sset Master 5%-"
  XF86AudioLowerVolume

"amixer sset Master toggle"
  XF86AudioMute

"playerctl play-pause"
  XF86AudioPlay

"playerctl next"
  XF86AudioNext

"playerctl previous"
  XF86AudioPrev

"flameshot gui"
  Mod4+Shift + s

"flameshot screen -c"
  Print

"flameshot screen -p ~/pictures/screenshots"
  Control + Print
