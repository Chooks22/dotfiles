{
	"Debug": {
		"prefix": "debug",
		"body": "console.debug($1)"
	},
	"Log": {
		"prefix": "log",
		"body": "console.log($1)"
	},
	"Info": {
		"prefix": "info",
		"body": "console.info($1)"
	},
	"Export": {
		"prefix": "xp",
		"body": "export "
	},
	"Export Default": {
		"prefix": "xpd",
		"body": "export default "
	},
	"Function Declaration": {
		"prefix": "fnd",
		"body": [
			"function $1($2)$4 {",
			"\t$3",
			"}"
		]
	},
	"Function Expression": {
		"prefix": "fn",
		"body": [
			"let $1 = ($2)$4 => {",
			"\t$3",
			"}"
		]
	},
	"Async Function Declaration": {
		"prefix": "afnd",
		"body": [
			"async function $1($2)$4 {",
			"\t$3",
			"}"
		]
	},
	"Async Function Expression": {
		"prefix": "afn",
		"body": [
			"let $1 = ($2)$4 => {",
			"\t$3",
			"}"
		]
	}
}